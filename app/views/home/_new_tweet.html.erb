<%=form_with(model: @tweet, local: true) do |f|%>
        <div class="">
            <%=f.label "Tweet"%>
            <br>
            <%=f.text_area :message, class: "form-control", placeholder: "What's on your mind?!"%>
        </div>
        <button type="submit" class="btn btn-primary my-3">Tweet</button>
<%end%>

